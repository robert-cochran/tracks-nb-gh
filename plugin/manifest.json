{

	"manifest_version": 2,
	"name": "tracks",
	"description": "manage local youtube tracks list",
	"version": "1.0",

	"icons": { "48": "icons/gold-album.png" },

	"browser_specific_settings": {
		"gecko": {
			"id": "tracks@extension.org",
			"strict_min_version": "50.0"
		}
	},

	"browser_action": {
		"default_icon": {
			"16": "icons/gold-album.png",
			"32": "icons/gold-album.png"
		}
	},

	"background": {
		"scripts": ["./native-messaging/background/sendNativeMessage.js"]
	},

	"permissions": [
		"*://127.0.0.1/*",
		"*://localhost/*",
		"tabs",
		"nativeMessaging"
	]
  
	

}
