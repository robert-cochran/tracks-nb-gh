"content_scripts": [
	{
		"matches": ["<all_urls>"],
		"js": ["./server/sendUrlToServer.js"]
	}
],

"browser_action": {
	"default_icon": {
		"16": "icons/gold-album.png",
		"32": "icons/gold-album.png"
	},
	"default_popup": "./popup/choose_page.html",
	"browser_style": true
},

"permissions": [
	"*://127.0.0.1/*",
	"*://localhost/*",
	"tabs",
	"nativeMessaging"
]